    "@types/eslint": "^7.2.2",
    "@types/glob": "^7.1.3",
    "@types/graceful-fs": "4.1.3",
    "@types/parse-gitignore": "^1.0.0",
    "baretest": "^2.0.0",
    "beachball": "^2.22.2",
    "fast-glob": "^3.2.4",
    "gh-pages": "^3.1.0",
    "husky": "^8.0.1",
    "lage": "^0.41.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.0.0-pre.58",
    "typedoc": "^0.19.1",
    "typedoc-plugin-markdown": "^2.4.2",
    "typescript": "^3.9.7",
    "vuepress": "1.5.4"
